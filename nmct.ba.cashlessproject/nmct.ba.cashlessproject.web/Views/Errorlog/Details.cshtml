@model IEnumerable<nmct.ba.cashlessproject.model.it.Errorlog>

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>
@Html.ActionLink("Back to List", "Index")
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegisterID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Timestamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Message)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stacktrace)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RegisterID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Timestamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Message)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Stacktrace)
        </td>
        <td>
        </td>
    </tr>
}

</table>
@Html.ActionLink("Back to List", "Index")