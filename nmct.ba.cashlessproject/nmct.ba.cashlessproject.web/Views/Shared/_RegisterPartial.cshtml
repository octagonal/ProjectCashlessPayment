@model IEnumerable<nmct.ba.cashlessproject.model.it.Register>

@{
    ViewBag.Title = "Registers";
}

<h2>Registers</h2>

<p>
    @Html.ActionLink("Create New", "Create", "Register")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegisterName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Device)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PurchaseDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExpiresDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RegisterName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Device)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PurchaseDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExpiresDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", "Register", new { id=item.ID }, null) |
            @Html.ActionLink("Details", "Details", "Register", new { id = item.ID }, null) |
            @Html.ActionLink("Delete", "Delete", "Register", new { id = item.ID }, null)
        </td>
    </tr>
}

</table>
